{
  "name": "@fubaratico/date-time-picker",
  "version": "0.1.1",
  "description": "A React 19 date and time picker component",
  "type": "module",
  "main": "./dist/datetime-picker.cjs.js",
  "module": "./dist/datetime-picker.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/datetime-picker.es.js",
      "require": "./dist/datetime-picker.cjs.js"
    },
    "./styles.css": "./dist/date-picker.css"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fubaratico/date-time-picker.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite -c vite.config.js",
    "storybook": "concurrently 'pnpm storybook:css' 'storybook dev -p 6006'",
    "storybook:css": "rm ./src/storybook.css && tailwindcss -w -i ./src/index.css -o ./src/storybook.css",
    "build": "rm -rf dist && rollup -c rollup.config.js && pnpm build:css",
    "build:css": "tailwindcss -i ./src/index.css -o ./dist/date-picker.css --minify",
    "test": "TZ=UTC jest",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc -p ./ --noEmit",
    "yalc:publish": "./yalc-dev.sh publish",
    "yalc:watch": "./yalc-dev.sh watch",
    "yalc:push": "./yalc-dev.sh push",
    "yalc:remove": "./yalc-dev.sh remove",
    "prepare": "husky",
    "prerelease": "pnpm lint && pnpm type-check && pnpm test && pnpm build",
    "release": "pnpm prerelease && pnpm build && pnpm publish --access public"
  },
  "packageManager": "pnpm@10.0.0",
  "keywords": [
    "react",
    "date-picker",
    "time-picker",
    "date-time-picker",
    "date-range-picker",
    "component"
  ],
  "author": "Fubaratico <contact@fubaratico.com>",
  "license": "MIT",
  "peerDependencies": {
    "@mona-health/react-input-mask": "3.0.3",
    "clsx": "^2.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-transition-group": "^4.4.5",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.3.0"
  },
  "engines": {
    "node": ">=22.11.0",
    "pnpm": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/traverse": "^7.27.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.21.0",
    "@mona-health/react-input-mask": "^3.0.3",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-dynamic-import-vars": "^2.1.5",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-designs": "^8.2.1",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-styling-webpack": "1.0.1",
    "@storybook/blocks": "^8.6.12",
    "@storybook/builder-vite": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/theming": "^8.6.14",
    "@storybook/types": "^8.6.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-transition-group": "^4.4.12",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^9.1.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "glob": "^11.0.2",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^15.5.2",
    "mockdate": "^3.0.5",
    "postcss": "^8.4.0",
    "postcss-prefix-selector": "^2.1.1",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-transition-group": "^4.4.5",
    "rollup": "^4.40.2",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "storybook": "^8.6.12",
    "storybook-dark-mode": "^4.0.2",
    "stylelint-config-standard": "^38.0.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
